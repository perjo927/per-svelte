{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "parcel-plugin-svelte": "^1.2.1",
    "rimraf": "^2.6.2",
    "svelte": "^2.6.0"
  },
  "scripts": {
    "build": "rimraf dist && parcel build ./public/index.html --public-url ./",
